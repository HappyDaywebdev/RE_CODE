

for (let i = 1; i <= 5; i++) {
    console.log("for loop count:",i);
}

let count = 1;
while (count <= 5)
{
    console.log("while loop count:",count);
    count++;
    console.log("while loop count:",count);
    
}

let a = 10;

do {
    console.log("do loop count:",a);
    a++;
} while (a <= 5);